<p id="notice"><%= notice %></p>

<h1>Nihss Forms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nihss_forms.each do |nihss_form| %>
      <tr>
        <td><%= link_to 'Show', nihss_form %></td>
        <td><%= link_to 'Edit', edit_nihss_form_path(nihss_form) %></td>
        <td><%= link_to 'Destroy', nihss_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nihss Form', new_nihss_form_path %>
